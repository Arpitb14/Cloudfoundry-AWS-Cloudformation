{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Private Subnet(s)",

  "Parameters": {
    "DeploymentName": {
      "Type": "String",
      "Description": "Deployment Name",
      "MinLength": "1",
      "MaxLength": "16",
      "AllowedPattern": "^([A-Za-z0-9]+[A-Za-z0-9-]?)+[A-Za-z0-9]$",
      "ConstraintDescription": "Must be a valid domain label matching /^([A-Za-z0-9]+[A-Za-z0-9-])+[A-Za-z0-9]$/"
    },

    "PrivateAz1Cidr": {
      "Type": "String",
      "Default": "10.0.40.0/24",
      "Description": "Private aZ1 CIDR",
      "AllowedPattern": "^([0-9]{1,3}\\.){3}[0-9]{1,3}/[0-9]{1,2}$",
      "ConstraintDescription": "Valid CIDR"
    },
    "PrivateAz2Cidr": {
      "Type": "String",
      "Default": "10.0.41.0/24",
      "Description": "Private AZ2 CIDR",
      "AllowedPattern": "^([0-9]{1,3}\\.){3}[0-9]{1,3}/[0-9]{1,2}$",
      "ConstraintDescription": "Valid CIDR"
    },
    "PrivateAz3Cidr": {
      "Type": "String",
      "Default": "10.0.42.0/24",
      "Description": "Private AZ3 CIDR",
      "AllowedPattern": "^([0-9]{1,3}\\.){3}[0-9]{1,3}/[0-9]{1,2}$",
      "ConstraintDescription": "Valid CIDR"
    },
    "MultiAz": {
      "Type": "String",
      "Default": "true",
      "Description": "Deploy multiple AZs",
      "AllowedValues": [ "true", "false" ]
    }
  },

  "Outputs": {
    "PrivateAz1Subnet": {
      "Description": "Private AZ1 Subnet",
      "Value": { "Ref": "PrivateAz1Subnet" },
      "Export": { "Name": { "Fn::Sub": "${DeploymentName}-PrivateAz1Subnet" } }
    },
    "PrivateAz2Subnet": {
      "Description": "Private AZ2 Subnet",
      "Condition": "MultiAz",
      "Value": { "Ref": "PrivateAz2Subnet" },
      "Export": { "Name": { "Fn::Sub": "${DeploymentName}-PrivateAz2Subnet" } }
    },
    "PrivateAz3Subnet": {
      "Description": "Private AZ3 Subnet",
      "Condition": "MultiAz",
      "Value": { "Ref": "PrivateAz3Subnet" },
      "Export": { "Name": { "Fn::Sub": "${DeploymentName}-PrivateAz3Subnet" } }
    },
    "PrivateAz1Cidr": {
      "Description": "Private AZ1 Cidr",
      "Value": { "Ref": "PrivateAz1Cidr" },
      "Export": { "Name": { "Fn::Sub": "${DeploymentName}-PrivateAz1Cidr" } }
    },
    "PrivateAz2Cidr": {
      "Description": "Private AZ2 Cidr",
      "Condition": "MultiAz",
      "Value": { "Ref": "PrivateAz2Cidr" },
      "Export": { "Name": { "Fn::Sub": "${DeploymentName}-PrivateAz2Cidr" } }
    },
    "PrivateAz3Cidr": {
      "Description": "Private AZ3 Cidr",
      "Condition": "MultiAz",
      "Value": { "Ref": "PrivateAz3Cidr" },
      "Export": { "Name": { "Fn::Sub": "${DeploymentName}-PrivateAz3Cidr" } }
    },
    "PrivateAz1CidrPrefix": {
      "Description": "Private AZ1 CIDR Prefix",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] }
          ]
        ]
      }
    },
    "PrivateAz2CidrPrefix": {
      "Description": "Private AZ2 CIDR Prefix",
      "Condition": "MultiAz",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] }
          ]
        ]
      }
    },
    "PrivateAz3CidrPrefix": {
      "Description": "Private AZ3 CIDR",
      "Condition": "MultiAz",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] }
          ]
        ]
      }
    },
    "PrivateAz1DefaultRoute": {
      "Description": "DMZ AZ1 Default Route",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            {
              "Fn::FindInMap": [
                "Route",
                { "Fn::Select": [ "1", { "Fn::Split" : [ "/", { "Ref": "PrivateAz1Cidr" } ] } ] },
                { "Fn::Select": [ "0", { "Fn::Split": [ "/", [ { "Fn::Select": [ "3", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] } ] ] } ] }
              ]
            }
          ]
        ]
      }
    },
    "PrivateAz2DefaultRoute": {
      "Description": "DMZ AZ1 Default Route",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            {
              "Fn::FindInMap": [
                "Route",
                { "Fn::Select": [ "1", { "Fn::Split" : [ "/", { "Ref": "PrivateAz2Cidr" } ] } ] },
                { "Fn::Select": [ "0", { "Fn::Split": [ "/", [ { "Fn::Select": [ "3", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] } ] ] } ] }
              ]
            }
          ]
        ]
      }
    },
    "PrivateAz3DefaultRoute": {
      "Description": "DMZ AZ1 Default Route",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            {
              "Fn::FindInMap": [
                "Route",
                { "Fn::Select": [ "1", { "Fn::Split" : [ "/", { "Ref": "PrivateAz3Cidr" } ] } ] },
                { "Fn::Select": [ "0", { "Fn::Split": [ "/", [ { "Fn::Select": [ "3", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] } ] ] } ] }
              ]
            }
          ]
        ]
      }
    },
    "PrivateAz1ReservedStart": {
      "Description": "DMZ AZ1 Reserved Start",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            {
              "Fn::FindInMap": [
                "ReservedStart",
                { "Fn::Select": [ "1", { "Fn::Split" : [ "/", { "Ref": "PrivateAz1Cidr" } ] } ] },
                { "Fn::Select": [ "0", { "Fn::Split": [ "/", [ { "Fn::Select": [ "3", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] } ] ] } ] }
              ]
            }
          ]
        ]
      }
    },
    "PrivateAz2ReservedStart": {
      "Description": "DMZ AZ1 Reserved Start",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            {
              "Fn::FindInMap": [
                "ReservedStart",
                { "Fn::Select": [ "1", { "Fn::Split" : [ "/", { "Ref": "PrivateAz2Cidr" } ] } ] },
                { "Fn::Select": [ "0", { "Fn::Split": [ "/", [ { "Fn::Select": [ "3", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] } ] ] } ] }
              ]
            }
          ]
        ]
      }
    },
    "PrivateAz3ReservedStart": {
      "Description": "DMZ AZ1 Reserved Start",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            {
              "Fn::FindInMap": [
                "ReservedStart",
                { "Fn::Select": [ "1", { "Fn::Split" : [ "/", { "Ref": "PrivateAz3Cidr" } ] } ] },
                { "Fn::Select": [ "0", { "Fn::Split": [ "/", [ { "Fn::Select": [ "3", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] } ] ] } ] }
              ]
            }
          ]
        ]
      }
    },
    "PrivateAz1ReservedStop": {
      "Description": "DMZ AZ1 Reserved Stop",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] },
            {
              "Fn::FindInMap": [
                "ReservedStop",
                { "Fn::Select": [ "1", { "Fn::Split" : [ "/", { "Ref": "PrivateAz1Cidr" } ] } ] },
                { "Fn::Select": [ "0", { "Fn::Split": [ "/", [ { "Fn::Select": [ "3", { "Fn::Split": [ ".", { "Ref": "PrivateAz1Cidr" } ] } ] } ] ] } ] }
              ]
            }
          ]
        ]
      }
    },
    "PrivateAz2ReservedStop": {
      "Description": "DMZ AZ1 Reserved Stop",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] },
            {
              "Fn::FindInMap": [
                "ReservedStop",
                { "Fn::Select": [ "1", { "Fn::Split" : [ "/", { "Ref": "PrivateAz2Cidr" } ] } ] },
                { "Fn::Select": [ "0", { "Fn::Split": [ "/", [ { "Fn::Select": [ "3", { "Fn::Split": [ ".", { "Ref": "PrivateAz2Cidr" } ] } ] } ] ] } ] }
              ]
            }
          ]
        ]
      }
    },
    "PrivateAz3ReservedStop": {
      "Description": "DMZ AZ1 Reserved Stop",
      "Value": {
	"Fn::Join": [
          ".", [
            { "Fn::Select": [ "0", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            { "Fn::Select": [ "1", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            { "Fn::Select": [ "2", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] },
            {
              "Fn::FindInMap": [
                "ReservedStop",
                { "Fn::Select": [ "1", { "Fn::Split" : [ "/", { "Ref": "PrivateAz3Cidr" } ] } ] },
                { "Fn::Select": [ "0", { "Fn::Split": [ "/", [ { "Fn::Select": [ "3", { "Fn::Split": [ ".", { "Ref": "PrivateAz3Cidr" } ] } ] } ] ] } ] }
              ]
            }
          ]
        ]
      }
    },

    "PrivateSecurityGroup": {
      "Description": "Private Security Groups",
      "Value": { "Ref": "PrivateSecurityGroup" },
      "Export": { "Name": { "Fn::Sub": "${DeploymentName}-PrivateSecurityGroup" } }
    }
  },

  "Mappings": {
    "Route": {
      "24": { "0": "1" },
      "25": { "0": "1", "128": "129" },
      "26": { "0": "1", "192": "193", "128": "129", "64": "65"},
      "27": { "0": "1", "224": "225", "192": "193", "160": "161", "128": "129", "96": "97", "64": "65", "32": "33"},
      "28": { "0": "1", "240": "241", "224": "225", "208": "209", "192": "193", "176": "177", "160": "161", "144": "145", "128": "129", "112": "113", "96": "97", "80": "81", "64": "65", "48": "49", "32": "33", "16": "17" }
    },
    "ReservedStart": {
      "24": { "0": "4" },
      "25": { "128": "132", "0": "4" },
      "26": { "192": "196", "128": "132", "64": "68", "0": "4" },
      "27": { "224": "228", "192": "196", "160": "164", "128": "132", "96": "100", "64": "68", "32": "36", "0": "4" },
      "28": { "240": "244", "224": "228", "208": "212", "192": "196", "176": "180", "160": "164", "144": "148", "128": "132", "112": "116", "96": "100", "80": "84", "64": "68", "48": "52", "32": "36", "16": "20","0": "4" }
    },
    "ReservedStop": {
      "24": { "0": "1" },
      "25": { "128": "129", "0": "127" },
      "26": { "192": "193", "128": "129", "64": "65", "0": "127" },
      "27": { "224": "254", "192": "222", "160": "190", "128": "158", "96": "126", "64": "94", "32": "62", "0": "30" },
      "28": { "240": "254", "224": "238", "208": "222", "192": "206", "176": "190", "160": "174", "144": "158", "128": "142", "112": "126", "96": "110", "80": "94", "64": "78", "48": "62", "32": "46", "16": "30", "0": "14" }
    }
  },

  "Conditions": {
    "MultiAz": { "Fn::Equals": [ { "Ref": "MultiAz" }, "true" ] }
  },

  "Resources": {
    "StackProtection": {
      "Type": "AWS::CloudFormation::Stack",
      "Properties": {
        "Parameters": {
          "StackArn": { "Ref": "AWS::StackId" },
          "StackProtectionGroup": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-StackProtectionGroup" } },
          "StackDeleteAllowDeny": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-StackDeleteAllowDeny" } },
          "StackUpdateAllowDeny": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-StackUpdateAllowDeny" } }
        },
        "TemplateURL": {
          "Fn::Join": [
            "/", [
              { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-preamble-TemplatesBucketHttpUrl" } },
              "Templates",
              "Stack-Protection.json"
            ]
          ]
        }
      }
    },  
    "PrivateAz1Subnet": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "AvailabilityZone": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-AwsAvailabilityZone1" } },
        "CidrBlock": { "Ref": "PrivateAz1Cidr" },
        "MapPublicIpOnLaunch": false,
        "VpcId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-CloudFoundryVpc" } },
        "Tags": [
          { "Key": "Name", "Value": { "Fn::Sub": "${DeploymentName}:PrivateAz1Subnet" } },
          { "Key": "Deployment", "Value": { "Ref": "DeploymentName" } }
        ]
      }
    },
    "PrivateAz2Subnet": {
      "Type": "AWS::EC2::Subnet",
      "Condition": "MultiAz",
      "Properties": {
        "AvailabilityZone": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-AwsAvailabilityZone2" } },
        "CidrBlock": { "Ref": "PrivateAz2Cidr" },
        "MapPublicIpOnLaunch": false,
        "VpcId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-CloudFoundryVpc" } },
        "Tags": [
          { "Key": "Name", "Value": { "Fn::Sub": "${DeploymentName}:PrivateAz2Subnet" } },
          { "Key": "Deployment", "Value": { "Ref": "DeploymentName" } }
        ]
      }
    },
    "PrivateAz3Subnet": {
      "Type": "AWS::EC2::Subnet",
      "Condition": "MultiAz",
      "Properties": {
        "AvailabilityZone": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-AwsAvailabilityZone3" } },
        "CidrBlock": { "Ref": "PrivateAz3Cidr" },
        "MapPublicIpOnLaunch": false,
        "VpcId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-CloudFoundryVpc" } },
        "Tags": [
          { "Key": "Name", "Value": { "Fn::Sub": "${DeploymentName}:PrivateAz3Subnet" } },
          { "Key": "Deployment", "Value": { "Ref": "DeploymentName" } }
        ]
      }
    },
    "PrivateAz1SubnetRouteTableAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": { "Ref": "PrivateAz1Subnet" },
        "RouteTableId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-NatAz1RouteTable" } }
      }
    },
    "PrivateAz2SubnetRouteTableAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Condition": "MultiAz",
      "Properties": {
        "SubnetId": { "Ref": "PrivateAz2Subnet" },
        "RouteTableId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-NatAz2RouteTable" } }
      }
    },
    "PrivateAz3SubnetRouteTableAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Condition": "MultiAz",
      "Properties": {
        "SubnetId": { "Ref": "PrivateAz3Subnet" },
        "RouteTableId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-NatAz3RouteTable" } }
      }
    },

    "PrivateSecurityGroup": {
      "Type": "AWS::EC2::SecurityGroup",
      "Properties": {
        "GroupDescription": "Private In Out",
        "VpcId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-CloudFoundryVpc" } },
        "SecurityGroupEgress": [
          { "CidrIp": "0.0.0.0/0", "FromPort": "0", "ToPort": "65535", "IpProtocol": "-1" }
        ],
        "SecurityGroupIngress": [
          { "CidrIp": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-VpcCidr" } }, "FromPort": "0", "ToPort": "65535" , "IpProtocol": "-1" }
        ],
        "Tags": [
          { "Key": "Name", "Value": { "Fn::Sub": "${DeploymentName}:PrivateSecurityGroup" } },
          { "Key": "Deployment", "Value": { "Ref": "DeploymentName" } }
        ]
      }
    }
  }
}
