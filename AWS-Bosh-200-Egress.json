        "FromPort": "25555",
        "ToPort": "25555" ,
        "IpProtocol": "tcp"
      }
    },
    "DirectorAz2DirectorElb25555AccessEgress": {
      "Type": "AWS::EC2::SecurityGroupEgress",
      "Condition": "DeployElb",
      "Properties": {
        "GroupId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-DirectorElbSecurityGroup" } },
        "CidrIp": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-DirectorAz2Cidr" } },
        "FromPort": "25555",
        "ToPort": "25555" ,
        "IpProtocol": "tcp"
      }
    },
    
    "PrivateAz1Director22AccessEgress": {
      "Type": "AWS::EC2::SecurityGroupEgress",
      "Properties": {
        "GroupId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-DirectorInstanceSecurityGroup" } },
        "CidrIp": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-PrivateAz1Cidr" } },
        "FromPort": "22",
        "ToPort": "22" ,
        "IpProtocol": "tcp"
      }
    },
    "PrivateAz2Director22AccessEgress": {
      "Type": "AWS::EC2::SecurityGroupEgress",
      "Condition": "MultiAz",
      "Properties": {
        "GroupId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-DirectorInstanceSecurityGroup" } },
        "CidrIp": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-PrivateAz2Cidr" } },
        "FromPort": "22",
        "ToPort": "22" ,
        "IpProtocol": "tcp"
      }
    },
    "PrivateAz3Director22AccessEgress": {
      "Type": "AWS::EC2::SecurityGroupEgress",
      "Condition": "MultiAz",
      "Properties": {
        "GroupId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-DirectorInstanceSecurityGroup" } },
