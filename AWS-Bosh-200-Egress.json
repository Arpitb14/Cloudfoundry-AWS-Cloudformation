      }
    },

    "PrivateToExternalSyslogPortAz1SecurityGroupEgress": {
      "Type": "AWS::EC2::SecurityGroupEgress",
      "Condition": "WhitelistExternalSyslogAz1Cidr",
      "Properties": {
        "GroupId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-PrivateSecurityGroup" } },
