        "FromPort": "6868",
        "ToPort": "6868" ,
        "IpProtocol": "tcp"
      }
    },
    "DirectorAz2DirectorElb6868AccessEgress": {
      "Type": "AWS::EC2::SecurityGroupEgress",
      "Condition": "DeployElb",
      "Properties": {
        "GroupId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-DirectorElbSecurityGroup" } },
        "CidrIp": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-DirectorAz2Cidr" } },
        "FromPort": "6868",
        "ToPort": "6868" ,
        "IpProtocol": "tcp"
      }
    },
    "DirectorAz1DirectorElb25555AccessEgress": {
      "Type": "AWS::EC2::SecurityGroupEgress",
      "Condition": "DeployElb",
      "Properties": {
        "GroupId": { "Fn::ImportValue": { "Fn::Sub": "${DeploymentName}-DirectorElbSecurityGroup" } },
